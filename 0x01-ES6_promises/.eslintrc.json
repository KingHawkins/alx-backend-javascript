{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
	"no-console": "off"
    },
    "overrides":[
        {
    "files": "*.js",
    "excludedFiles": "babel.config.js"
    }
  ]
}
